{
    "name": "Shiny App",
    "slug": "shinyapp",
    "category": "serve",
    "table_field": {
        "url": "https://{{ release }}.{{ global.domain }}"
    },
    "description": "",
    "priority": "400",
    "settings": {
        "flavor": "one",
        "logs": ["serve"],
        "default_values": {
          "port": "80",
          "targetport": "3838"
        },
        "permissions": {
            "public": {"value":"false", "option": "true"},
            "project": {"value":"true", "option": "true"},
            "private": {"value":"false", "option": "true"}
        },
        "appconfig": {
          "meta": {
            "title": "Configurations"
          },
          "image": {
            "type": "string",
            "default": "registry/repository/image:tag",
            "title": "Image"
          }
        }
    }
} 